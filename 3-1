#include<stdio.h>
#include<stdlib.h> 
#include<conio.h> 
#pragma warning(disable : 4996)

int main(void)
{
    int i, j;

    for (i = 1; i < 10; i++)
    {
        system("cls");
        for (j = 1; j < 10; j++)
        {
            printf("%d*%d=%d ", i, j, i * j);
        }

        printf("\n아무키나 누르세요");
        getch();
    }
    return 0;
}


